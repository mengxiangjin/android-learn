<?xml version="1.0" encoding="utf-8"?>
<layer-list xmlns:android="http://schemas.android.com/apk/res/android">

    <item>
        <shape>
            <solid android:color="#FFEEF4FA" />
        </shape>
    </item>

    <item
        android:top="-80dp"
        android:left="20dp"
        android:right="20dp"
        >
        <shape
            android:shape="oval"
            android:useLevel="true">
            <solid android:color="#ffffff" />
        </shape>
    </item>

    <item
        android:top="100dp">
        <shape
            android:shape="rectangle">

            <solid android:color="#ffffff"/>

        </shape>

    </item>

</layer-list>
