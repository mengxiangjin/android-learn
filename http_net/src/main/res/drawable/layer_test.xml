<?xml version="1.0" encoding="utf-8"?>
<layer-list xmlns:android="http://schemas.android.com/apk/res/android">

    <item>
        <shape android:shape="oval">
            <stroke android:color="#06F99B" android:width="10dp"/>
        </shape>
    </item>

    <item android:left="10dp"
        android:top="10dp"
        android:bottom="10dp"
        android:right="10dp">
        <shape android:shape="oval">
            <stroke android:color="#010302" android:width="20dp"/>
            <solid android:color="@color/white"/>
        </shape>
    </item>


</layer-list>